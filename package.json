{
  "name": "quonx-hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && wait-on http://localhost:5173 && electron .",
    "build": "vite build && npm run build:electron",
    "build:electron": "tsc -p tsconfig.electron.json",
    "preview": "vite preview",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.example.aistudio",
    "productName": "AI Studio",
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "@monaco-editor/react": "^4.7.0",
    "d3": "^7.9.0",
    "dotenv": "^17.2.2",
    "monaco-editor": "^0.53.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "three": "^0.167.0",
    "vite-plugin-monaco-editor": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "concurrently": "^9.2.1",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.5"
  }
}
